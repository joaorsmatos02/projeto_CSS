<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.Label?>

<?import javafx.scene.layout.VBox?>
<StackPane fx:id="pane" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.desktopapp.consult_projects.ProjectDetailsController">

	<HBox fx:id="topHbox" alignment="top_left">
		<children>
			<Button fx:id="goBack" text="Voltar" onAction="#goBack" />
			<Label fx:id="title" text="Consultar projeto de lei"></Label>
		</children>
	</HBox>

	<VBox fx:id="mainVbox" alignment="center">
		<children>
			<Label fx:id="id" text="Id:"></Label>
			<Label fx:id="theme" text="Tema:"></Label>
			<Label fx:id="projectTitle" text="Titulo:"></Label>
			<Label fx:id="description" text="Descricao:"></Label>
			<Label fx:id="expiration" text="Data de validade:"></Label>
			<Label fx:id="nrSupporters" text="Nr apoiantes:"></Label>
			<Label fx:id="delegate" text="Delegado:"></Label>
		</children>
	</VBox>


</StackPane>
